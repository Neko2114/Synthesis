<Page x:Class="Synthesis.PresetPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Synthesis"
      mc:Ignorable="d" 
      d:DesignHeight="330" d:DesignWidth="556"
      Title="modPage">
    <Grid x:Name="MainGrid" Margin="10,10,10,10">
        <Separator HorizontalAlignment="Left" Height="5" Margin="15,74,0,0" VerticalAlignment="Top" Width="422" Background="White" Foreground="White"/>
        <Label x:Name="PageHeader" Content="" FontFamily="Arial" FontSize="48" Margin="10,10,10,236" FontStyle="Italic"/>
        <Button x:Name="restartBtn" Content="↻" HorizontalAlignment="Left" Height="37" Margin="484,10,0,0" VerticalAlignment="Top" Width="42" FontSize="24" FontFamily="MS UI Gothic" Click="restartClick"/>
        <Button x:Name="finish" Content="finish" HorizontalAlignment="Center" Height="10" Margin="0,275,0,0" VerticalAlignment="Top" Width="114" Click="finishBtn"/>
        <Grid x:Name="buttonGrid" HorizontalAlignment="Left" Height="191" Margin="38,79,0,0" VerticalAlignment="Top" Width="490">
        </Grid>
        <Button x:Name="next" Content="next" HorizontalAlignment="Center" Height="10" Margin="0,275,0,0" VerticalAlignment="Top" Width="114" Click="nextBtn"/>
    </Grid>
</Page>
